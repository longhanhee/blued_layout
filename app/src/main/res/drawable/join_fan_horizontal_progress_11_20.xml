<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="@color/live_fan_progress_11_20"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/live_fan_progress_11_20" android:endColor="@color/live_fan_progress_11_20" android:angle="0" android:centerY="0.75" android:centerColor="@color/live_fan_progress_11_20"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/live_fan_progress_11_20" android:endColor="@color/live_fan_progress_11_20" android:angle="0"/>
            </shape>
        </scale>
    </item>
</layer-list>
