<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#73010618" android:endColor="#73131532"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item android:width="7dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#a8ff8158"/>
            <corners android:topLeftRadius="4dp" android:bottomLeftRadius="4dp"/>
            <gradient android:startColor="#d4f97022" android:endColor="#d4d32267" android:angle="270"/>
        </shape>
    </item>
</layer-list>
