<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2dp"/>
            <solid android:color="#131423"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2dp"/>
                <gradient android:startColor="#e0e1f2" android:endColor="#e0e1f2" android:angle="270" android:centerY="0.75" android:centerColor="#e0e1f2"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <scale android:drawable="@drawable/progress_horizontal_inside_bg_black" android:scaleWidth="100%"/>
    </item>
</layer-list>
